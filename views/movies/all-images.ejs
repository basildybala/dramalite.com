<%- include('../partials/header'); -%>
<title><%= movie.name %> Hd Images and movie stills -Dramalite</title>
<meta name="description" content="<%= movie.engname %> movie hd images and posters download and view">
<meta name="keywords" content="<%= movie.engname %> Images, <%= movie.engname %> <%= movie.category %> Movie Images and Poster, <%= movie.engname %> <%= movie.year %>, <%= movie.engname %> movie Photos, <%= movie.actorname1 %> <%= movie.engname %> Images, <%= movie.actorname1 %> <%= movie.engname %> Movie Images and stills, <%= movie.engname %> images Download"/>
    <link rel="stylesheet" href="../../public/css/movie.css">

    <div class="other-sec">
        <div class="left-blue-header">
            <h2><%= movie.name %> Images</h2>
        </div>
        <div class="images">
            <% if(movie.images.length >0) { %>
            <% for(var i=0; i<movie.images.length; i++){%>
            <div class="image">
                <a href="/image/download?imgUrl=<%= movie.images[i] %>"><img data-src="/<%= movie.images[i] %>" alt="" class="lazy-photo"></a>
            </div>
            <% } %>
            <% }else{ %>
                <p>Images not uploaded</p>
            <% } %>
        </div>
    </div>





<%- include('../partials/footer'); -%>